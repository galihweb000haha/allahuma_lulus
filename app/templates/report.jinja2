{%extends "layout_dashboard.jinja2" %}
{%block pagestyles %}

{% endblock %}
{% block content %}
<div class="container-xxl flex-grow-1 container-p-y">
    <div class="card">
        <div class="card-header d-flex align-items-center justify-content-between">
            <h5 class="mb-0">Laporan hasil prediksi</h5>
            <a href="/excel/download"s class="btn btn-sm btn-secondary float-end"><i class="bx bx-export"></i> Download Excel</a>
        </div>
        <div class="table-responsive text-nowrap">
            <table class="table">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Nama</th>
                        <th>Semester</th>
                        <th>Prodi</th>
                        <th>Relevan</th>
                    </tr>
                </thead>
                <tbody class="table-border-bottom-0">
                    {% for data in datas %}
                        <tr>
                            <td>{{ loop.index }}</td>
                            <td>{{data.nama}}</td>
                            <td>{{data.semester}}</td>
                            <td>{{data.prodi}}</td>
                            <td>{{data.relevan}}</td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
{% endblock %}
